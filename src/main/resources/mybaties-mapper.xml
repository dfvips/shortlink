<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dreamfly.shortlink.mybaties.Mapper">
    <select id="search" resultType="com.dreamfly.shortlink.entity.db">
		SELECT * from db where u_id = #{u_id}
	</select>
    <insert id="insert">
		INSERT ignore  INTO db (o_url,url,u_id) VALUES (#{o_url},#{url},#{u_id})
	</insert>
</mapper>